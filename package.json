{
  "devDependencies": {
    "@11ty/eleventy": "^3.0.0",
    "@mightyplow/eleventy-plugin-cache-buster": "^1.1.3",
    "wrangler": "^3.83.0"
  },
  "scripts": {
    "clean": "rm -rf _site",
    "copy-assets": "mkdir -p _site && cp -r public/* _site/",
    "dev": "npm run clean && npx @11ty/eleventy --port 8000 --serve",
    "build": "npm run copy-assets && npx @11ty/eleventy",
    "cf": "npm run clean && npm run build && awk -F'=' '{print \"-b \"$1\"=\"$2}' .env | tr '\n' ' ' | xargs npx wrangler pages dev _site --port 8000"
  },
  "dependencies": {
    "dotenv": "^16.4.5"
  }
}
